<!DOCTYPE html>
<html lang="en">
<head>
  <%- include('../Partials/head'); %>
</head>
<body class="container">

<header>
  <%- include('../Partials/header', {Page: 'index'}); %>
</header>

<main>
  <div class="jumbotron">
    <h1>This is great</h1>
    <p>Welcome to templating using EJS</p>

    <h2>Passed Value In Redering</h2>
    <p><%= line %></p>

    <ul>
      <% users.forEach(function(user) { %>
        <li>
          <strong><%= user.name %></strong>
          representing <%= user.company %>,
          born <%= user.age %>
        </li>
      <% }); %>
    </ul>
  </div>
</main>

<footer>
  <%- include('../Partials/footer'); %>
</footer>

</body>
</html>